<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/blankFragment">

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.hw1_temperature_convert.BlankFragment"
        android:label="BlankFragment"
        tools:layout="@layout/fragment_blank">
        <action
            android:id="@+id/action_blankFragment_to_blankFragment2"
            app:destination="@id/blankFragment2"
            app:popUpTo="@id/blankFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/blankFragment2"
        android:name="com.example.hw1_temperature_convert.BlankFragment2"
        android:label="BlankFragment2"
        tools:layout="@layout/fragment_blank2"/>
    <fragment
        android:id="@+id/about"
        android:name="com.example.hw1_temperature_convert.About"
        android:label="About"
        tools:layout="@layout/fragment_about"/>
    <fragment
        android:id="@+id/rulesFragment"
        android:name="com.example.hw1_temperature_convert.RulesFragment"
        android:label="fragment_rules"
        tools:layout="@layout/fragment_rules" />

</navigation>